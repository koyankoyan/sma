      "source": [
        "# Fed rate https://fred.stlouisfed.org/series/FEDFUNDS\n",
        "fedfunds = pdr.DataReader(\"FEDFUNDS\", \"fred\", start=start)\n",
        "fedfunds.tail()"
      ],
      "metadata": {
        "colab": {
