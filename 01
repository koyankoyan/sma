      "cell_type": "code",
      "source": [
        "import requests\n",
        "import pandas as pd\n",
        "\n",
        "# Add your CoinMarketCap API key here\n",
        "api_key = '912b8b47-eac1-458e-8893-326d88b023b4'\n",
        "\n",
        "# Define the API endpoint for cryptocurrency data\n",
        "url = 'https://pro-api.coinmarketcap.com/v1/cryptocurrency/listings/latest'\n",
        "\n",
        "# Define the parameters for the API request\n",
        "parameters = {\n",
        "  'start': '500',\n",
        "  'limit': '789', # Adjust this to get more or fewer cryptocurrencies\n",
        "  'convert': 'USD'\n",
        "}\n",
        "\n",
        "# Set up the headers with your API key\n",
        "headers = {\n",
        "  'Accepts': 'application/json',\n",
        "  'X-CMC_PRO_API_KEY': api_key,\n",
        "}\n",
        "\n",
        "# Send the request to the API\n",
        "response = requests.get(url, headers=headers, params=parameters)\n",
        "data = response.json()\n",
        "\n",
        "# Convert the response to a DataFrame for easier analysis\n",
        "crypto_data = pd.json_normalize(data['data'])\n",
        "\n",
        "# Display the first few rows of the DataFrame\n",
        "crypto_data[['name', 'symbol', 'quote.USD.price', 'quote.USD.percent_change_24h']]\n"
